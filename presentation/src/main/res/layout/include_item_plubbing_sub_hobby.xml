        android:layout_height="wrap_content"
        android:background="@drawable/bg_rectangle_filled_e1e1fa_radius_4"
        >

        <TextView
            android:id="@+id/text_view_sub_hobby_name"
            app:layout_constraintTop_toTopOf="parent"
